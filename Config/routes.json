{

    "": {
        "route": "Meumodulo"
    },
    "teste-url-amigavel/{[0-9]*}": {
        "route": "Meumodulo/index/index",
        "id": "$1"
    },
    "{[A-z-_0-9]+}/{[A-z-_0-9]*}/{[A-z-_0-9]*}": {
        "route": "$1/$2/$3"
    },
    "{[A-z-_0-9]+}/{[A-z-_0-9]+}": {
        "route": "$1/$2"
    },
    "{[A-z-_0-9]+}": {
        "route": "$1"
    }

}
